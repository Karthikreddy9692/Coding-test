package vamshi;

import java.util.Scanner;
//Write a program to check whether a given number is divisible by both 5 and 11 or not.

public class Divisible {

	public static void main(String[] args) {
	Scanner kk=new Scanner(System.in);
	// User input 
	System.out.println("Enter a Number:");
	int r=kk.nextInt();
	// Check the number is divisible by both 5 and 11
	if(r%5==0 && r%11==0) {
		System.out.println("Yes it is divisible by both 5 and 11");
	
	}
	else {
		System.out.println("It is not divisible by 5 & 11");
	}
	}

}
